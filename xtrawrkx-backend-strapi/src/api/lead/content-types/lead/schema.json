{
    "kind": "collectionType",
    "collectionName": "leads",
    "info": {
        "singularName": "lead",
        "pluralName": "leads",
        "displayName": "Lead",
        "description": "Potential customers and prospects"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "leadName": {
            "type": "string",
            "required": true
        },
        "companyName": {
            "type": "string",
            "required": true
        },
        "email": {
            "type": "email",
            "required": true
        },
        "phone": {
            "type": "string"
        },
        "website": {
            "type": "string"
        },
        "industry": {
            "type": "string"
        },
        "size": {
            "type": "string"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "NEW",
                "CONTACTED",
                "QUALIFIED",
                "LOST",
                "CONVERTED"
            ],
            "default": "NEW"
        },
        "score": {
            "type": "integer"
        },
        "source": {
            "type": "enumeration",
            "enum": [
                "EXTENSION",
                "MANUAL",
                "IMPORT",
                "WEBSITE",
                "REFERRAL"
            ],
            "default": "MANUAL"
        },
        "assignedTo": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "ownedLeads"
        },
        "notes": {
            "type": "text"
        },
        "lastContactDate": {
            "type": "datetime"
        },
        "convertedAccount": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::account.account",
            "inversedBy": "convertedLeads"
        },
        "convertedContact": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::contact.contact",
            "inversedBy": "convertedLeads"
        },
        "emailLogs": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::email-log.email-log",
            "mappedBy": "lead"
        },
        "activities": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::activity.activity",
            "mappedBy": "lead"
        }
    }
}