{
    "kind": "collectionType",
    "collectionName": "projects",
    "info": {
        "singularName": "project",
        "pluralName": "projects",
        "displayName": "Project",
        "description": "Client projects and deliverables"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "deal": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::deal.deal",
            "inversedBy": "projects"
        },
        "account": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::account.account",
            "inversedBy": "projects"
        },
        "name": {
            "type": "string",
            "required": true
        },
        "description": {
            "type": "text"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "PLANNING",
                "ACTIVE",
                "IN_PROGRESS",
                "COMPLETED",
                "ON_HOLD",
                "CANCELLED"
            ],
            "default": "PLANNING"
        },
        "startDate": {
            "type": "datetime"
        },
        "endDate": {
            "type": "datetime"
        },
        "budget": {
            "type": "decimal"
        },
        "spent": {
            "type": "decimal"
        },
        "color": {
            "type": "string"
        },
        "icon": {
            "type": "string"
        },
        "projectManager": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "managedProjects"
        },
        "activities": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::activity.activity",
            "mappedBy": "project"
        },
        "timeEntries": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::time-entry.time-entry",
            "mappedBy": "project"
        },
        "files": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::file.file",
            "mappedBy": "project"
        }
    }
}



