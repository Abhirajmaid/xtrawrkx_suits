{
    "kind": "collectionType",
    "collectionName": "lead_imports",
    "info": {
        "singularName": "lead-import",
        "pluralName": "lead-imports",
        "displayName": "Lead Import",
        "description": "Lead import operations and tracking"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "fileName": {
            "type": "string",
            "required": true
        },
        "importType": {
            "type": "enumeration",
            "enum": [
                "CSV",
                "EXCEL"
            ],
            "required": true
        },
        "totalRecords": {
            "type": "integer",
            "required": true
        },
        "processedRecords": {
            "type": "integer",
            "required": true
        },
        "successfulRecords": {
            "type": "integer",
            "required": true
        },
        "failedRecords": {
            "type": "integer",
            "required": true
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "PENDING",
                "PROCESSING",
                "COMPLETED",
                "FAILED"
            ],
            "default": "PENDING"
        },
        "importedBy": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "leadImports"
        },
        "completedAt": {
            "type": "datetime"
        }
    }
}



