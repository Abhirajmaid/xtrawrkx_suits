{
    "kind": "collectionType",
    "collectionName": "community_memberships",
    "info": {
        "singularName": "community-membership",
        "pluralName": "community-memberships",
        "displayName": "Community Membership",
        "description": "Active memberships of clients in different communities"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "clientAccount": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::client-account.client-account",
            "inversedBy": "communityMemberships"
        },
        "community": {
            "type": "enumeration",
            "enum": [
                "XEN",
                "XEVFIN",
                "XEVTG",
                "XDD"
            ],
            "required": true
        },
        "membershipType": {
            "type": "enumeration",
            "enum": [
                "FREE",
                "PREMIUM",
                "ENTERPRISE"
            ],
            "default": "FREE"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "ACTIVE",
                "INACTIVE",
                "SUSPENDED",
                "EXPIRED"
            ],
            "default": "ACTIVE"
        },
        "joinedAt": {
            "type": "datetime",
            "required": true
        },
        "expiresAt": {
            "type": "datetime"
        },
        "lastActivityAt": {
            "type": "datetime"
        },
        "membershipData": {
            "type": "json"
        },
        "benefits": {
            "type": "json"
        },
        "restrictions": {
            "type": "json"
        }
    }
}