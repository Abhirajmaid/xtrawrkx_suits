{
    "kind": "collectionType",
    "collectionName": "reports",
    "info": {
        "singularName": "report",
        "pluralName": "reports",
        "displayName": "Report",
        "description": "Business reports and analytics"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "type": {
            "type": "enumeration",
            "enum": [
                "SALES",
                "PROJECT",
                "TIME",
                "REVENUE",
                "CUSTOM"
            ],
            "required": true
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "DRAFT",
                "SCHEDULED",
                "GENERATED",
                "ERROR"
            ],
            "default": "DRAFT"
        },
        "createdBy": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "createdReports"
        }
    }
}



