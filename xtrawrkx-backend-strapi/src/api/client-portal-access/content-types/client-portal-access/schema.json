{
    "kind": "collectionType",
    "collectionName": "client_portal_access",
    "info": {
        "singularName": "client-portal-access",
        "pluralName": "client-portal-accesses",
        "displayName": "Client Portal Access",
        "description": "Client portal access credentials and permissions"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "contact": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::contact.contact",
            "inversedBy": "portalAccess"
        },
        "password": {
            "type": "password",
            "required": true
        },
        "isActive": {
            "type": "boolean",
            "default": true
        },
        "accessLevel": {
            "type": "enumeration",
            "enum": [
                "view",
                "comment",
                "upload"
            ],
            "default": "view"
        },
        "lastLogin": {
            "type": "datetime"
        },
        "account": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::account.account",
            "inversedBy": "portalAccess"
        }
    }
}



