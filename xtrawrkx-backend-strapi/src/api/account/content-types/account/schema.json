{
    "kind": "collectionType",
    "collectionName": "accounts",
    "info": {
        "singularName": "account",
        "pluralName": "accounts",
        "displayName": "Account",
        "description": "Client companies and business accounts"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "companyName": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "industry": {
            "type": "string",
            "required": true
        },
        "type": {
            "type": "enumeration",
            "enum": [
                "CUSTOMER",
                "PROSPECT",
                "PARTNER",
                "VENDOR"
            ],
            "default": "CUSTOMER"
        },
        "website": {
            "type": "string"
        },
        "phone": {
            "type": "string"
        },
        "email": {
            "type": "email"
        },
        "address": {
            "type": "text"
        },
        "city": {
            "type": "string"
        },
        "state": {
            "type": "string"
        },
        "zipCode": {
            "type": "string"
        },
        "country": {
            "type": "string"
        },
        "employees": {
            "type": "string"
        },
        "description": {
            "type": "text"
        },
        "founded": {
            "type": "string"
        },
        "linkedIn": {
            "type": "string"
        },
        "twitter": {
            "type": "string"
        },
        "revenue": {
            "type": "decimal"
        },
        "healthScore": {
            "type": "integer"
        },
        "password": {
            "type": "password",
            "required": true
        },
        "passwordResetToken": {
            "type": "string"
        },
        "passwordResetExpires": {
            "type": "datetime"
        },
        "emailVerificationToken": {
            "type": "string"
        },
        "emailVerified": {
            "type": "boolean",
            "default": false
        },
        "lastLoginAt": {
            "type": "datetime"
        },
        "isActive": {
            "type": "boolean",
            "default": true
        },
        "owner": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "ownedAccounts"
        },
        "source": {
            "type": "enumeration",
            "enum": [
                "ONBOARDING",
                "LEAD_CONVERSION",
                "MANUAL",
                "IMPORT"
            ],
            "default": "MANUAL"
        },
        "lastActivity": {
            "type": "datetime"
        },
        "contacts": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::contact.contact",
            "mappedBy": "account"
        },
        "deals": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::deal.deal",
            "mappedBy": "account"
        },
        "projects": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::project.project",
            "mappedBy": "account"
        },
        "activities": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::activity.activity",
            "mappedBy": "account"
        },
        "proposals": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::proposal.proposal",
            "mappedBy": "account"
        },
        "contracts": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::contract.contract",
            "mappedBy": "account"
        },
        "invoices": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::invoice.invoice",
            "mappedBy": "account"
        },
        "portalAccess": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::client-portal-access.client-portal-access",
            "mappedBy": "account"
        },
        "files": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::file.file",
            "mappedBy": "account"
        },
        "convertedLeads": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::lead.lead",
            "mappedBy": "convertedAccount"
        }
    }
}