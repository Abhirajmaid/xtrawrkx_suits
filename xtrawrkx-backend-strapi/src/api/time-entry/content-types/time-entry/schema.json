{
    "kind": "collectionType",
    "collectionName": "time_entries",
    "info": {
        "singularName": "time-entry",
        "pluralName": "time-entries",
        "displayName": "Time Entry",
        "description": "Time tracking entries for projects and tasks"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "project": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::project.project",
            "inversedBy": "timeEntries"
        },
        "task": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::task.task",
            "inversedBy": "timeEntries"
        },
        "user": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "timeEntries"
        },
        "description": {
            "type": "string",
            "required": true
        },
        "hours": {
            "type": "decimal",
            "required": true
        },
        "date": {
            "type": "datetime",
            "required": true
        },
        "hourlyRate": {
            "type": "decimal"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "DRAFT",
                "APPROVED",
                "BILLED"
            ],
            "default": "DRAFT"
        }
    }
}



