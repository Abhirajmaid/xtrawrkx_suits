        "email": {
            "type": "email",
            "required": true,
            "unique": true
        },
        "firstName": {
            "type": "string",
            "required": true
        },
        "lastName": {
            "type": "string",
            "required": true
        },
        "phone": {
            "type": "string"
        },
        "location": {
            "type": "string"
        },
        "timezone": {
            "type": "string",
            "default": "America/New_York"
        },
        "bio": {
            "type": "text"
        },
        "avatar": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images"
            ]
        },
        "password": {
            "type": "password",
            "required": true
        },
        "firebaseUid": {
            "type": "string",
            "unique": true
        },
        "passwordResetToken": {
            "type": "string"
        },
        "passwordResetExpires": {
            "type": "datetime"
        },
        "emailVerificationToken": {
            "type": "string"
        },
        "invitationToken": {
            "type": "string"
        },
        "invitationExpires": {
            "type": "datetime"
        },
        "invitedBy": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user"
        },
        "primaryRole": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::user-role.user-role",
            "inversedBy": "primaryUsers"
        },
        "department": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::department.department",
            "inversedBy": "users"
        },
        "isActive": {
            "type": "boolean",
            "default": true
        },
        "emailVerified": {
            "type": "boolean",
            "default": false
        },
        "lastLoginAt": {
            "type": "datetime"
        },
        "authProvider": {
            "type": "enumeration",
            "enum": [
                "PASSWORD",
                "FIREBASE",
                "HYBRID"
            ],
            "default": "PASSWORD"
        },
        "ownedAccounts": {
