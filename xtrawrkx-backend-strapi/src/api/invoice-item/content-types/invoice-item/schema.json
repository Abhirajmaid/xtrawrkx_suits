{
    "kind": "collectionType",
    "collectionName": "invoice_items",
    "info": {
        "singularName": "invoice-item",
        "pluralName": "invoice-items",
        "displayName": "Invoice Item",
        "description": "Individual line items within invoices"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "invoice": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::invoice.invoice",
            "inversedBy": "items"
        },
        "description": {
            "type": "string",
            "required": true
        },
        "quantity": {
            "type": "decimal",
            "required": true
        },
        "unitPrice": {
            "type": "decimal",
            "required": true
        },
        "totalPrice": {
            "type": "decimal",
            "required": true
        }
    }
}



