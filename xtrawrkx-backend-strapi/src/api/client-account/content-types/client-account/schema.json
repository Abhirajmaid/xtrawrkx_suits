{
    "kind": "collectionType",
    "collectionName": "client_accounts",
    "info": {
        "singularName": "client-account",
        "pluralName": "client-accounts",
        "displayName": "Client Account",
        "description": "Converted client companies and business accounts"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "companyName": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "industry": {
            "type": "string",
            "required": true
        },
        "website": {
            "type": "string"
        },
        "phone": {
            "type": "string"
        },
        "email": {
            "type": "email"
        },
        "address": {
            "type": "text"
        },
        "city": {
            "type": "string"
        },
        "state": {
            "type": "string"
        },
        "country": {
            "type": "string"
        },
        "zipCode": {
            "type": "string"
        },
        "employees": {
            "type": "string"
        },
        "founded": {
            "type": "string"
        },
        "description": {
            "type": "text"
        },
        "type": {
            "type": "enumeration",
            "enum": [
                "CUSTOMER",
                "PARTNER",
                "VENDOR"
            ],
            "default": "CUSTOMER"
        },
        "revenue": {
            "type": "decimal"
        },
        "healthScore": {
            "type": "integer",
            "min": 0,
            "max": 100,
            "default": 75
        },
        "conversionDate": {
            "type": "datetime"
        },
        "lastActivity": {
            "type": "string"
        },
        "lastActivityDate": {
            "type": "datetime"
        },
        "linkedIn": {
            "type": "string"
        },
        "twitter": {
            "type": "string"
        },
        "notes": {
            "type": "text"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "ACTIVE",
                "INACTIVE",
                "CHURNED",
                "ON_HOLD"
            ],
            "default": "ACTIVE"
        },
        "contacts": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::contact.contact",
            "mappedBy": "clientAccount"
        },
        "activities": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::activity.activity",
            "mappedBy": "clientAccount"
        },
        "chatMessages": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::chat-message.chat-message",
            "mappedBy": "clientAccount"
        },
        "tasks": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::task.task",
            "mappedBy": "clientAccount"
        },
        "deals": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::deal.deal",
            "mappedBy": "clientAccount"
        },
        "projects": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::project.project",
            "mappedBy": "clientAccount"
        },
        "invoices": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::invoice.invoice",
            "mappedBy": "clientAccount"
        },
        "proposals": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::proposal.proposal",
            "mappedBy": "clientAccount"
        },
        "convertedFromLead": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::lead-company.lead-company",
            "inversedBy": "convertedAccount"
        },
        "accountManager": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user"
        },
        "password": {
            "type": "password",
            "required": false
        },
        "emailVerified": {
            "type": "boolean",
            "default": false
        },
        "isActive": {
            "type": "boolean",
            "default": true
        },
        "source": {
            "type": "enumeration",
            "enum": [
                "ONBOARDING",
                "MANUAL",
                "IMPORT",
                "API"
            ],
            "default": "MANUAL"
        },
        "onboardingCompleted": {
            "type": "boolean",
            "default": false
        },
        "onboardingCompletedAt": {
            "type": "datetime"
        },
        "onboardingData": {
            "type": "json"
        },
        "selectedCommunities": {
            "type": "json"
        },
        "communitySubmissions": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::community-submission.community-submission",
            "mappedBy": "clientAccount"
        },
        "communityMemberships": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::community-membership.community-membership",
            "mappedBy": "clientAccount"
        }
    }
}