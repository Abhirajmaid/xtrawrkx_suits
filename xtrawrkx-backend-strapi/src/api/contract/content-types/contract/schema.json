{
    "kind": "collectionType",
    "collectionName": "contracts",
    "info": {
        "singularName": "contract",
        "pluralName": "contracts",
        "displayName": "Contract",
        "description": "Legal contracts and agreements"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "proposal": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::proposal.proposal",
            "inversedBy": "contract"
        },
        "deal": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::deal.deal",
            "inversedBy": "contracts"
        },
        "account": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::account.account",
            "inversedBy": "contracts"
        },
        "contact": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::contact.contact",
            "inversedBy": "contracts"
        },
        "title": {
            "type": "string",
            "required": true
        },
        "contractContent": {
            "type": "richtext",
            "required": true
        },
        "contractValue": {
            "type": "decimal",
            "required": true
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "DRAFT",
                "SENT",
                "SIGNED",
                "ACTIVE",
                "EXPIRED",
                "CANCELLED"
            ],
            "default": "DRAFT"
        },
        "signedByContact": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::contact.contact",
            "inversedBy": "contractsSignedBy"
        },
        "createdBy": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "createdContracts"
        },
        "signedAt": {
            "type": "datetime"
        },
        "startDate": {
            "type": "datetime"
        },
        "endDate": {
            "type": "datetime"
        }
    }
}



