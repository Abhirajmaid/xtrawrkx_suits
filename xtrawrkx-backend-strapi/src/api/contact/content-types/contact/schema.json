{
    "kind": "collectionType",
    "collectionName": "contacts",
    "info": {
        "singularName": "contact",
        "pluralName": "contacts",
        "displayName": "Contact",
        "description": "Contact persons within client companies"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "account": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::account.account",
            "inversedBy": "contacts"
        },
        "firstName": {
            "type": "string",
            "required": true
        },
        "lastName": {
            "type": "string",
            "required": true
        },
        "email": {
            "type": "email",
            "required": true,
            "unique": true
        },
        "phone": {
            "type": "string"
        },
        "title": {
            "type": "string"
        },
        "department": {
            "type": "string"
        },
        "role": {
            "type": "enumeration",
            "enum": [
                "DECISION_MAKER",
                "INFLUENCER",
                "USER",
                "GATEKEEPER",
                "PRIMARY_CONTACT"
            ],
            "default": "USER"
        },
        "portalAccessLevel": {
            "type": "enumeration",
            "enum": [
                "FULL_ACCESS",
                "PROJECT_VIEW",
                "INVOICE_VIEW",
                "READ_ONLY",
                "NO_ACCESS"
            ],
            "default": "READ_ONLY"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "ACTIVE",
                "INACTIVE",
                "LEFT_COMPANY"
            ],
            "default": "ACTIVE"
        },
        "address": {
            "type": "text"
        },
        "birthday": {
            "type": "date"
        },
        "profileImage": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images"
            ]
        },
        "linkedIn": {
            "type": "string"
        },
        "twitter": {
            "type": "string"
        },
        "assignedTo": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "ownedContacts"
        },
        "lastContactDate": {
            "type": "datetime"
        },
        "source": {
            "type": "enumeration",
            "enum": [
                "ONBOARDING",
                "LEAD_CONVERSION",
                "MANUAL",
                "EXTENSION",
                "IMPORT"
            ],
            "default": "MANUAL"
        },
        "deals": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::deal.deal",
            "mappedBy": "contact"
        },
        "activities": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::activity.activity",
            "mappedBy": "contact"
        },
        "communityMemberships": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::community-membership.community-membership",
            "mappedBy": "contact"
        },
        "portalAccess": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::client-portal-access.client-portal-access",
            "mappedBy": "contact"
        },
        "files": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::file.file",
            "mappedBy": "contact"
        },
        "convertedLeads": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::lead.lead",
            "mappedBy": "convertedContact"
        },
        "proposals": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::proposal.proposal",
            "mappedBy": "contact"
        },
        "proposalsSentTo": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::proposal.proposal",
            "mappedBy": "sentToContact"
        },
        "contracts": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::contract.contract",
            "mappedBy": "contact"
        },
        "contractsSignedBy": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::contract.contract",
            "mappedBy": "signedByContact"
        }
    }
}