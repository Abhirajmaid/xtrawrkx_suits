{
    "kind": "collectionType",
    "collectionName": "proposals",
    "info": {
        "singularName": "proposal",
        "pluralName": "proposals",
        "displayName": "Proposal",
        "description": "Business proposals and quotes"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "deal": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::deal.deal",
            "inversedBy": "proposals"
        },
        "clientAccount": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::client-account.client-account",
            "inversedBy": "proposals"
        },
        "contact": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::contact.contact",
            "inversedBy": "proposals"
        },
        "title": {
            "type": "string",
            "required": true
        },
        "proposalContent": {
            "type": "richtext",
            "required": true
        },
        "validUntil": {
            "type": "datetime"
        },
        "status": {
            "type": "enumeration",
            "enum": [
                "DRAFT",
                "SENT",
                "ACCEPTED",
                "REJECTED",
                "EXPIRED"
            ],
            "default": "DRAFT"
        },
        "sentToContact": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::contact.contact",
            "inversedBy": "proposalsSentTo"
        },
        "createdBy": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::xtrawrkx-user.xtrawrkx-user",
            "inversedBy": "createdProposals"
        },
        "sentAt": {
            "type": "datetime"
        },
        "respondedAt": {
            "type": "datetime"
        },
        "contract": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::contract.contract",
            "mappedBy": "proposal"
        },
        "emailLog": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::email-log.email-log",
            "inversedBy": "proposals"
        }
    }
}



