{
  "name": "xtrawrkx-backend-strapi",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "strapi develop",
    "develop": "strapi develop",
    "start": "strapi start",
    "clean:cache": "node scripts/clean-cache.js",
    "start:clean": "npm run clean:cache && npm start",
    "strapi": "strapi",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry",
    "setup:dev": "node scripts/setup-env.js && copy .env.development .env && echo ✅ Switched to development environment",
    "setup:prod": "node scripts/setup-env.js && copy .env.production .env && echo ✅ Switched to production environment",
    "dev:local": "npm run setup:dev && npm run develop",
    "start:prod": "npm run setup:prod && npm run start",
    "env:check": "echo Current environment: && type .env | findstr NODE_ENV",
    "debug:env": "node scripts/debug-env.js"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.27.0",
